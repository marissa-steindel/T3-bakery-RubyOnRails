<h1>Province Member</h1>
  <%= link_to "Province Collection", provinces_index_path%>

  <h2><%= @province.name%></h2>

  <% unless @province.GST.nil? || @province.GST == 0 %>
    <p>GST: <%= @province.GST.to_i %>%</p>
  <% end %>
    <% unless @province.PST.nil? || @province.PST == 0 %>
    <p>PST: <%= @province.PST.to_i %>%</p>
  <% end %>
    <% unless @province.HST.nil? || @province.HST == 0 %>
    <p>HST: <%= @province.HST.to_i %>%</p>
  <% end %>
